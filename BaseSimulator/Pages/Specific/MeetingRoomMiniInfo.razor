<table class="table">
    <tbody>
        <tr>
            <th scope="row">Additional Friend Slots</th>
            <td>@this.Game.BaseData.MeetingData.Phases[this.Game.LevelToPhase(this.Level)].FriendSlotInc</td>
        </tr>
        <tr>
            <th scope="row">Power Consumed</th>
            <td>@(-this.Room.Phases[this.Game.LevelToPhase(this.Level)].Electricity)</td>
        </tr>
    </tbody>
</table>

@code {
    [CascadingParameter] public Game Game { get; set; }
    [CascadingParameter] public Room Room { get; set; }
    [Parameter] public int Level { get; set; }
}
